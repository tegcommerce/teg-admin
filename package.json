    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.4",
    "redux-immutable": "^4.0.0",
