{
  "name": "tegcommerce-admin",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "dev:server": "webpack-dashboard -- webpack-dev-server --watch --config build/webpack.dev.config.js --mode=development",
    "prod:server": "webpack-dev-server --watch --config build/webpack.prod.config.js --mode=production",
    "prod:build": "webpack --config build/webpack.prod.config.js --mode=production",
    "test": "jest",
    "test:codecov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:lint": "eslint src/**/*.test.js src/**/*.test.jsx",
    "lint": "eslint --format node_modules/eslint-friendly-formatter src/**/*.js src/**/*.jsx && npm run test:lint",
    "lint:fix": "eslint --format node_modules/eslint-friendly-formatter src/**/*.{js,jsx} --fix",
    "start": "npm run dev:server",
    "build": "npm run prod:build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tegcommerce/tegcommerce-admin"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
